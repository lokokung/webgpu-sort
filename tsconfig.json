{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "lib": ["dom", "es2020"],
    "module": "esnext",
    "outDir": "./out",
    "allowJs": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": false,
    "noUnusedLocals": true,
    "allowUnreachableCode": true,
    "moduleResolution": "node",
    "esModuleInterop": false,
    "skipLibCheck": true,
    "target": "es2020",
    "types": ["@webgpu/types", "jquery"]
  },
  "files": ["src/sort.ts"],
}
